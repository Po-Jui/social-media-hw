nk>\r\n      </p>\r\n    </form>\r\n    <form v-if=\"!islogin\">\r\n      <h3>您已經登入!!!三秒後回到貼文頁面</h3>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { firebaseAuth } from \"@/config/firebaseConfig.js\";\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      user: {\r\n        email: \"\",\r\n        password: \"\"\r\n      },\r\n      islogin:false\r\n    };\r\n  },\r\n  created() {\r\n    firebaseAuth.onAuthStateChanged((user) => {\r\n      if (user != null) {\r\n        this.islogin = false;\r\n        setTimeout(() => {\r\n          this.$router.push('/posts')\r\n        }, 3000); \r\n      } else {\r\n        this.islogin = true;\r\n        \r\n      }\r\n    });\r\n  },\r\n  methods: {\r\n    userLogin() {\r\n      firebaseAuth\r\n        .signInWithEmailAndPassword(this.user.email, this.user.password)\r\n        .then(() => {\r\n          this.$router.push(\"/posts\");\r\n        })\r\n        .catch((error) => {\r\n          alert(error.message);\r\n        });\r\n    },\r\n  },\r\n};\r\n</script>\r\n"]}]}